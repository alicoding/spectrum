    "imager": "latest",
    "mongoose": "latest",
    "nunjucks": "latest",
    "notifier": "latest",
    "passport": "latest",
    "passport-facebook": "latest",
    "passport-github": "latest",
    "passport-google-oauth": "latest",
    "passport-local": "latest",
    "passport-twitter": "latest",
    "path": "latest",
    "underscore": "latest",
    "view-helpers": "latest"
  },
  "devDependencies": {
    "supertest": "latest",
    "should": "latest",
    "mocha": "latest",
    "nodemon": "latest"
  }
