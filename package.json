{
  "name": "Spectrum",
  "description": "A demo app illustrating the usage of express framework, mongoose orm, mongodb and stylus in node.js. The also illustrates how to properly organize your application",
  "keywords": [
    "express",
    "mongoose",
    "mongodb",
    "passport",
    "demo"
  ],
  "version": "3.0.0",
  "private": false,
  "author": "Madhusudhan Srinivasa <madhums8@gmail.com> (http://madhums.github.com)",
  "engines": {
    "node": "0.10.x",
    "npm":  "1.2.x"
  },
  "scripts": {
